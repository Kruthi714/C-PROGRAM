#include <stdio.h>
#include<stdlib.h>
void main()
{
     int n,i;
    int a[5];
   
    printf("enter the value of n: ");\
    scanf("%d\n",&n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    printf("array elements are: ");
    for(i=0;i<n;i++)
    {
       printf("%d\n",a[i]);
    }
   
 int sum=0;
 for(i=0;i<n;i++)
    {
        sum=sum+a[i];
    }
 printf("sum of array element is %d",sum);
   
}
